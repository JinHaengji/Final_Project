public class Main {

	public static void main(String[] args) 
	{
		File inputFile = new File("D://¡π¿€//file//wav//test5_1114.wav");
	    AudioInputStream audioInputStream = AudioSystem.getAudioInputStream(inputFile);
	    AudioSystem.write(audioInputStream, FLACFileWriter.FLAC, new File("D://¡π¿€//file//flac//temp5_1114.flac"));

	}

}
